<table>
  <thead>
    <tr>
      <th scope="col">First name</th>
      <th scope="col">Last name</th>
      <th scope="col">House</th>
      <th scope="col">Age</th>
    </tr>
  </thead>
<% for student in @students %>
  <tr>
  <td><%= student.first_name() %></td>
  <td><%= student.last_name() %></td>
  <td><%= student.house() %></td>
  <td><%= student.age() %></td>
  <tr />
<% end %>
</table>
